{
    "root": true,
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "global-require": "off",
        "import/no-extraneous-dependencies": "off",
        "no-console": "off",
        "arrow-body-style": "off"
    }
}
